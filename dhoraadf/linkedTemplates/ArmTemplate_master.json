{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dhoraadf"
		},
		"LNK_AZRSQL_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LNK_AZRSQL'"
		},
		"lnk_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lnk_adls'"
		},
		"LNK_AZRSQL_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dhora.database.windows.net"
		},
		"LNK_AZRSQL_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dhoradb"
		},
		"LNK_AZRSQL_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dhoraadm"
		},
		"lnk_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dhora.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dhoraadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LNK_AZRSQL_password": {
						"value": "[parameters('LNK_AZRSQL_password')]"
					},
					"lnk_adls_accountKey": {
						"value": "[parameters('lnk_adls_accountKey')]"
					},
					"LNK_AZRSQL_properties_typeProperties_server": {
						"value": "[parameters('LNK_AZRSQL_properties_typeProperties_server')]"
					},
					"LNK_AZRSQL_properties_typeProperties_database": {
						"value": "[parameters('LNK_AZRSQL_properties_typeProperties_database')]"
					},
					"LNK_AZRSQL_properties_typeProperties_userName": {
						"value": "[parameters('LNK_AZRSQL_properties_typeProperties_userName')]"
					},
					"lnk_adls_properties_typeProperties_url": {
						"value": "[parameters('lnk_adls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}